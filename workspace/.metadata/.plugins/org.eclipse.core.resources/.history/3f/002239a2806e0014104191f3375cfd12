package beerTycoonPack;

public class arrayPlay {

	public static void main(String[] args) {
		int myArray[] = {60,60,60,60,60};
		int i = 0;
		int tmp;
		int counter = 60;
		while(i < 20){
			i++;
			counter++;
			System.out.println(myArray[0] +" "+counter+" "+i);
			for ( int j = 4; j > 0; j-- ) {
				tmp = myArray[j];
				myArray[j-1] = tmp;
			}
			myArray[4] = counter;
		}
	}
}
